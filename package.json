    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "pont-engine": "^1.0.6",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.6.4",
    "web-vitals": "^1.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pretty-quick --staged",
      "npm run format",
      "npm run lint",
      "git add ."
    ]
  }
